<%= form_for @hiring_manager_note do |f| %>

  <div>
    <%= f.label :candidate_id %><br />
    <%= f.collection_select :candidate_id, Candidate.all, :id, :full_name %>
  </div>

  <div>
    <%= f.label :note_id %><br />
    <%= f.collection_select :note_id, Note.all, :id, :description %>
  </div>

  <div>
    <%= f.label :hiring_manager_id %><br />
    <%= f.collection_select :hiring_manager_id, HiringManager.all, :id, :full_name %>
  </div>


  <div>
    <%= f.submit %>
  </div>

<% end %>
