<h1>New Hiring Manager Job</h1>


<%= form_tag(hiring_manager_jobs_url, method: 'post') do %>


  <div>
    <%= label_tag :hiring_manager_id %><br />
    <%= number_field_tag :hiring_manager_id %>
  </div>

    <div>
    <%= label_tag :job_id %><br />
    <%= number_field_tag :job_id %>
  </div>

  
  <div>
    <%= submit_tag "Create Hiring Manager Job" %>
  </div>
<% end %>
