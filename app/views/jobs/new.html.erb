<h1>New Job</h1>


<%= form_tag(jobs_url, method: 'post') do %>


  <div>
    <%= label_tag :title %><br />
    <%= text_field_tag :title %>
  </div>

    <div>
    <%= label_tag :description %><br />
    <%= text_field_tag :description %>
  </div>

    <div>
    <%= label_tag :location %><br />
    <%= text_field_tag :location %>
  </div>

    <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

    <div>
    <%= label_tag :url %><br />
    <%= text_field_tag :url %>
  </div>

  
  <div>
    <%= submit_tag "Create Job" %>
  </div>
<% end %>
